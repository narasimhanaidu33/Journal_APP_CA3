<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApplicationCA3"
        tools:targetApi="31">
        <activity
            android:name="com.example.myapplicationca3.AddJournalActivity"
            android:theme="@style/Theme.AppCompat"
            android:exported="true" />
        <activity
            android:name="com.example.myapplicationca3.JournalList"
            android:theme="@style/Theme.AppCompat"
            android:exported="true" />
        <activity
            android:name="com.example.myapplicationca3.SignUpActivity"
            android:theme="@style/Theme.AppCompat"
            android:exported="true" />
        <activity
            android:name="com.example.myapplicationca3.MainActivityJournalApp"
            android:theme="@style/Theme.AppCompat"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.myapplicationca3.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.MyApplicationCA3">

        </activity>
    </application>

</manifest>